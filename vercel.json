{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/index.js"
        },
        {
            "src": "/sign_up",
            "dest": "/index.js",
            "methods": [
                "POST"
            ]
        },
        {
            "src": "/sign_in",
            "dest": "/index.js",
            "methods": [
                "POST"
            ]
        },
        {
            "src": "/alert_data",
            "dest": "/index.js",
            "methods": [
                "POST"
            ]
        },
        {
            "src": "/login_data",
            "dest": "/index.js",
            "methods": [
                "POST"
            ]
        },
        {
            "src": "/emer_cont",
            "dest": "/index.js",
            "methods": [
                "POST"
            ]
        },
        {
            "src": "/",
            "dest": "/public/index_.html"
        },
        {
            "src": "/(.*\\.(html|css|js|png|jpg|jpeg|gif|svg|webp|ico|json))",
            "dest": "/public/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}